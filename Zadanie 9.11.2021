import java.sql.SQLOutput;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
    double zaciatok;
    double ciel;
    double dsd;
    int pocetDni = 0;

        Scanner sc1=new Scanner(System.in);
        System.out.print("zadajte km pre 1. den: ");
